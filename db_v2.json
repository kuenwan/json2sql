{
	"name": "db_test",
	"version": "2",
	"definer": "test",
	"table": {
		"user": {
			"comment":"用户表",
			"sharding": 1,
			"fields": [
				{
					"name": "guid",
					"type": "uint64",
					"key": "1",
					"comment":"用户唯一id"
				},
				{
					"name": "account",
					"type": "string",
					"length": "80",
					"comment":"账号名称"
				},
				{
					"name": "createTime",
					"type": "int64",
					"comment":"创建时间"
				},
				{
					"name": "money",
					"type": "decimal",
					"length": "23",
					"point": "2",
					"comment":"持有的货币数量"
				}
			]
		},
		"timer_operate": {
			"comment":"时间表",
			"sharding": 1,
			"fields": [
				{
					"name": "id",
					"type": "int32",
					"key": "1",
					"comment":"唯一id"
				},
				{
					"name": "operateTime",
					"type": "int64",
					"comment":"操作时间"
				}
			]
		},
		"db_version": {
			"comment":"数据库版本表",
			"ignore": "1",
			"sharding": 1,
			"fields": [
				{
					"name": "id",
					"type": "uint32",
					"key": "1",
					"auto_increment": "1",
					"comment":"自增id"
				},
				{
					"name": "version",
					"type": "uint32",
					"comment":"版本号"
				},
				{
					"name": "update_time",
					"type": "timestamp",
					"comment":"更新时间"
				}
			]
		}
	}
}